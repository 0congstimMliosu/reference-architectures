{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "deploymentContext": { "type": "object" },
    "source": { "type": "object" },
    "context": { "type": "object" }
  },
  "variables": {
    "iisInstallerModuleUrl": "[uri(deployment().properties.templateLink.uri, 'iisaspnet.ps1.zip')]",
    "configurationFunction": "iisaspnet.ps1\\iisaspnet",
    "instance": {
      "settings": {
        "modulesUrl": "[variables('iisInstallerModuleUrl')]",
        "configurationFunction": "[variables('configurationFunction')]"
      },
      "protectedSettings": { }
    }
  },
  "resources": [ ],
  "outputs": {
    "settings": {
      "type": "object",
      "value": "[variables('instance')]"
    }
  }
}